<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Skills</title>
</head>
<body>
    <h1>Mad Skill</h1>

    <a href="/skills/new"> Add New Skill</a>
    <ul>
      <% skills.forEach(function(sk) { %>
        <li>
          <a href="/skills/<%=sk.id%> ">
          <%= sk.skill  %>
        </a>
            - 
          <%= sk.accomplished ? 'true' : 'not true' %>
        </li>
      <% }); %>
    </ul>
</body>
</html>